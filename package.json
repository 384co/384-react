    "start": "tsc -w ",
    "start:react": "cd test && pnpm start",
    "build": "lerna run --scope \"@384/*\" build",
    "build:all": "pnpm install && pnpm build && rm -rf node_modules",
    "test:v1": "concurrently --names \"UI,TSC,CYPRESS\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"pnpm start:react\" \"pnpm start\" \"pnpm cypress:open\"",
    "test:cli": "cypress run --spec \"cypress/e2e/384-tests/*.js\" --browser chrome",
    "cypress:open": "cypress open"
  },
