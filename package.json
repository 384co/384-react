{
  "name": "384-react",
  "version": "1.0.0",
  "description": "React components for 384 Lib",
  "types": "./dist/384.react.d.ts",
  "repository": "https://github.com/384co/384-react.git",
  "author": "matt384 <108557364+matt384@users.noreply.github.com>",
  "license": "GPL-3.0 license Only",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist && rm -rf build && rm -rf node_modules && rm -rf cypress && rm yarn.lock",
    "start": "tsc -w ",
    "build": "rm -rf dist && rm -rf build && tsc && esbuild src/index.ts --bundle --format=esm --sourcemap --outfile=dist/384react.js && dts-bundle-generator -o dist/384.react.d.ts src/index.ts",
    "test": "cypress run --spec \"cypress/e2e/384-tests/*.js\" --browser chrome",
    "cypress:open": "cypress open",
    "types": "dts-bundle-generator -o dist/index.d.ts src/index.ts"
  },
  "dependencies": {
    "dts-bundle-generator": "^9.0.0",
    "lib384": "git+https://github.com/384co/lib-proto-02.git",
    "lodash": "^4.17.21",
    "mobx": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.7",
    "@cypress/react": "^8.0.0",
    "@cypress/webpack-dev-server": "^3.6.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cypress": "^13.6.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}