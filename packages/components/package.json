    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=12.0.0"
  }
