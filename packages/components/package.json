{
  "name": "@384/components",
  "version": "1.0.0-alpha.0",
  "private": false,
  "author": "384 Inc",
  "description": "React components for 384 Lib",
  "keywords": [
    "react",
    "384"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/384co/384-react.git",
