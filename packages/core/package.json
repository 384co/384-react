{
  "name": "@384/core",
  "version": "1.0.0-alpha.0",
  "private": false,
  "author": "384 Inc",
  "description": "Mobx state management for 384 Lib",
  "keywords": [
    "react",
    "mobx",
    "state",
    "384"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/384co/384-react.git",
    "directory": "packages/core"
  },
  "main": "dist/cjs/index.js",
