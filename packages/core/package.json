    "mobx-react": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "@types/react-swipeable-views": "^0.13.5"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "lib384": "link:../../../lib384",
